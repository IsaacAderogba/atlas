{
  "compilerOptions": {
    "target": "es2016",
    "useDefineForClassFields": true,
    "module": "commonjs",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["src"]
}
