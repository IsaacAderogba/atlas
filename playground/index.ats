class Map[T] {
	record: Record[T] = { }

	add: (String, T) -> T = f(key, value) {
		return this.record.add(key, value)
	}
}

var stringMap = Map[String]()
print(stringMap.add("key", "foo"))

var numberMap = Map[Number]()
print(numberMap.add("key", 0))